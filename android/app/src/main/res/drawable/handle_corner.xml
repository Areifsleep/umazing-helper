<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    
    <!-- White fill -->
    <solid android:color="#FFFFFF" />
    
    <!-- Green border -->
    <stroke
        android:width="3dp"
        android:color="#4CAF50" />
    
    <!-- Circle size (will be 24dp x 24dp) -->
    <size
        android:width="24dp"
        android:height="24dp" />
    
</shape>
